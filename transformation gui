	Zap{
		StartCFrame=Root.CFrame,
		EndCFrame=asd*CF.N(0,4,0),
		ZapRotation = {-5,5},
		PartCount=10,
		StartSize = 4,
		EndSize = .5,
		Color = C3.N(1,1,0),
		DelayInc=5,
		Delay = 15,
	}
